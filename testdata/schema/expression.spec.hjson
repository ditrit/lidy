    "accept if known": [
      ["timestamp", {}]
      ["boolean", {}]
      ["float", {}]
      ["null", {}]
      ["any", {}]
      ["int", {}]
      ["str", {}]
    ]
    "reject if unknown": [
      ["cat", { contain: "cat" }]
      ["bar", { contain: "bar" }]
    ]
  }
  "check that checkers are used with the right signature": {
    "reject": [
      ["_map: word", {}]
      ["_map: str", {}]
