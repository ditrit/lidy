  "check for in.checker": {
    "accept valid forms": [
      [
        ["_in: []", {}]
        ["_in: [aaa]", {}]
        ["_in: [aaa, bbb]", {}]
        ["_in: [true, bbb]", {}]
        ["_in: [false, null, zz, 0.1]", {}]
        ["_in: [aaa, 22, \"22\"]", {}]
      ]
    ]
    "reject invalid forms": [
      [
        ["_in: [[]]", { contain: "_in" }]
        ["_in: [{}]", { contain: "_in" }]
        ["_in: [{ _map: {} }]", { contain: "_in" }]
        ["_in: 22", { contain: "_in" }]
        ["_in: {}", { contain: "_in" }]
        ["_in: aa", { contain: "_in" }]
        ["_in: .2", { contain: "_in" }]
        ["_in: true", { contain: "_in" }]
        ["_in: null", { contain: "_in" }]
